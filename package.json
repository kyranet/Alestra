{
  "name": "alestra",
  "version": "3.0.0",
  "description": "Canvas-Constructor helper bot and Soft-Sandbox Evaluator",
  "main": "dist/src/Alestra.js",
  "scripts": {
    "pm2:delete": "pm2 delete Alestra",
    "pm2:logs": "pm2 logs Alestra --lines=200",
    "pm2:restart": "pm2 restart Alestra",
    "pm2:start-harmony": "pm2 start dist/src/Alestra.js --node-args=\"--harmony\" --name=\"Alestra\"",
    "pm2:start": "pm2 start dist/src/Alestra.js --name=\"Alestra\"",
    "pm2:stop": "pm2 stop Alestra",
    "start": "node dist/src/Alestra.js",
    "lint": "eslint --ext ts src --fix",
    "test": "eslint --ext ts src",
    "test:lint": "eslint --ext ts src",
    "update": "yarn",
    "build": "tsc -b src",
	"build-clear": "rm -rf dist && tsc -b src",
    "watch": "tsc -p . -w",
    "dev": "yarn build && yarn start"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kyranet/Alestra.git"
  },
  "keywords": [
    "canvas-constructor",
    "canvas",
    "helper",
    "klasa"
  ],
  "contributors": [
    "kyranet"
  ],
  "private": true,
  "author": "kyranet <kyradiscord@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kyranet/Alestra/issues"
  },
  "homepage": "https://github.com/kyranet/Alestra#readme",
  "dependencies": {
    "@klasa/console": "^0.0.2",
    "@klasa/core": "github:dirigeants/core#build",
    "@klasa/stopwatch": "^0.0.1",
    "@klasa/utils": "^0.0.8",
    "@klasa/ws": "^0.0.13",
    "@skyra/decorators": "^1.0.0",
    "acorn": "^7.2.0",
    "acorn-numeric-separator": "^0.3.2",
    "canvas": "^2.6.1",
    "canvas-constructor": "^3.2.0",
    "eslint": "^7.1.0",
    "fs-nextra": "^0.5.1",
    "klasa": "github:dirigeants/klasa#build",
    "node-fetch": "^2.6.0",
    "typescript": "^3.9.3",
    "veza": "^1.1.0"
  },
  "devDependencies": {
    "@types/eslint": "^6.8.1",
    "@types/node": "^14.0.5",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint-config-bamboo": "^5.0.0"
  }
}
