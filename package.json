{
  "name": "alestra",
  "version": "3.0.0",
  "description": "Canvas-Constructor helper bot and Soft-Sandbox Evaluator",
  "main": "dist/src/Alestra.js",
  "scripts": {
    "lint": "tslint -p tsconfig.json src/**/*.ts --fix",
    "pm2:delete": "pm2 delete Alestra",
    "pm2:logs": "pm2 logs Alestra --lines=200",
    "pm2:restart": "pm2 restart Alestra",
    "pm2:start-harmony": "pm2 start dist/src/Alestra.js --node-args=\"--harmony\" --name=\"Alestra\"",
    "pm2:start": "pm2 start dist/src/Alestra.js --name=\"Alestra\"",
    "pm2:stop": "pm2 stop Alestra",
    "start": "node dist/src/Alestra.js",
    "test:lint": "tslint -p tsconfig.json src/**/*.ts",
    "update": "yarn",
    "compile": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kyranet/Alestra.git"
  },
  "keywords": [
    "Canvas-Constructor",
    "Canvas",
    "helper",
    "discord.js",
    "klasa"
  ],
  "contributors": [
    "kyranet",
    "yorkaargh"
  ],
  "private": true,
  "author": "kyranet <kyradiscord@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/kyranet/Alestra/issues"
  },
  "homepage": "https://github.com/kyranet/Alestra#readme",
  "dependencies": {
    "acorn": "^6.1.1",
    "acorn-numeric-separator": "^0.2.0",
    "canvas": "^2.4.1",
    "canvas-constructor": "3.0.1",
    "discord.js": "github:discordjs/discord.js",
    "eslint": "^5.15.3",
    "klasa": "github:dirigeants/klasa",
    "node-fetch": "^2.3.0",
    "typescript": "^3.4.1",
    "veza": "^0.4.0"
  },
  "devDependencies": {
    "@types/eslint": "^4.16.6",
    "@types/node": "^11.12.0",
    "@types/node-fetch": "^2.1.7",
    "eslint-config-klasa": "github:dirigeants/klasa-lint",
    "tslint": "^5.14.0"
  }
}
